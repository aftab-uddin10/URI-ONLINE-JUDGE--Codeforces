#include <iostream>
using namespace std;
int main() {
    int n,i,a;
    double sum=0;
    cin>>n;
    for(i=0;i<n;i++){
        cin>>a;
        sum+=a;
    }
    cout<<sum/n<<endl;

    return 0;
}
